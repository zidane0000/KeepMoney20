<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <padding
        android:top="2.5dp"
        android:right="2.5dp"
        android:bottom="2.5dp"
        android:left="2.5dp"/>
    <!-- 形狀大小 -->
    <size
        android:width="50dp"
        android:height="50dp" />
    <!-- 邊框線 -->
    <stroke
        android:width="1dp"
        android:color="@android:color/black"
        android:dashGap="2dp"
        android:dashWidth="5dp" />
</shape>
